---
import Layout from '../layouts/Layout.astro';
import Topvar from '../components/index/topvar.astro';
import Writetweet from '../components/index/writetweet.astro';
import Tweets from '../components/index/tweets.astro';

interface Props {
	class: string;
}
  
const { class: className } = Astro.props;
  
---

<Layout title="X - Home">
	<main class:list={[className]}>
		<Topvar />
		<Writetweet />
		<Tweets />
	</main>
</Layout>